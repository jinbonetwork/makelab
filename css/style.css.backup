.editor-content > * + * {
  margin-top: 1em;
}
.border {
  border: 1px solid #333333;
}
.banner {
  display: block;
  width: 100%;
}
img.banner,
.banner img {
  border: 1px solid #333333;
}
.banner img {
  width: 100%;
  height: auto;
}
.page-template-template-builder .site-content {
  overflow: hidden;
}
.builder-section > h1,
.builder-section > h2,
.builder-section > h3,
.builder-section > h4,
.builder-section > h5,
.builder-section > h6 {
  display: none;
}
.builder-section .builder-section-content .builder-text-image {
  border: 1px solid #333333;
}
.builder-section .builder-section-content .builder-text-content > * + * {
  margin-top: 1em;
}
.entry-thumbnail.post-header * {
  display: block;
  width: 100%;
  height: auto;
}
